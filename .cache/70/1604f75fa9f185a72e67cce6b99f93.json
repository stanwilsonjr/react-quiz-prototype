olumn":96},"source":"src/components/question.js","original":{"line":84,"column":39}},{"name":"activeQuestion","generated":{"line":113,"column":97},"source":"src/components/question.js","original":{"line":84,"column":53}},{"generated":{"line":113,"column":111},"source":"src/components/question.js","original":{"line":84,"column":15}},{"name":"answer","generated":{"line":113,"column":113},"source":"src/components/question.js","original":{"line":84,"column":69}},{"generated":{"line":113,"column":119},"source":"src/components/question.js","original":{"line":84,"column":15}},{"name":"explaination","generated":{"line":113,"column":120},"source":"src/components/question.js","original":{"line":84,"column":76}},{"generated":{"line":113,"column":132},"source":"src/components/question.js","original":{"line":84,"column":10}},{"generated":{"line":113,"column":133},"source":"src/components/question.js","original":{"line":83,"column":8}},{"generated":{"line":113,"column":134},"source":"src/components/question.js","original":{"line":74,"column":4}},{"generated":{"line":113,"column":135},"source":"src/components/question.js","original":{"line":73,"column":2}},{"generated":{"line":114,"column":0},"source":"src/components/question.js","original":{"line":90,"column":1}}],"sources":{"src/components/question.js":"import React, { useContext,useState} from \"react\";\nimport { Quizcontext } from \"./quizcontext\";\nimport styled from \"styled-components\";\n\n\nconst QuestionContainer = styled.section`\n    display:flex;\n    flex-direction: column;\n    & h2{\n      font-family: 'Open Sans', sans-serif;\n      font-weight:800;\n\n    }\n`;\nconst Answers = styled.section`\n    display: grid;\n    grid-template-columns: 2fr 2fr;\n    max-width: 500px;\n    flex-wrap: wrap;\n    column-gap:10px;\n    row-gap: 10px;\n    margin: 25px 0;\n    & div:nth-child(${props => props.correct}){\n      background: ${props => (props.submitted)? 'green': \"transparent\"};\n      color: ${props => (props.submitted)? '#fff': \"#333\"};\n\n    }\n    & div{\n        border: 1px solid #333;\n        border-radius: 7px;\n        padding: 7px 10px;\n        min-height: 50px;\n        cursor: pointer;\n        font-family: 'Open Sans', sans-serif;\n        font-weight:400;\n        transition: transform 0.5s linear;\n\n        &:hover{\n            background-color: #333;\n            color: #fff;\n            transform: translateY(-4px);\n        }\n    }\n`;\nconst ResultSection= styled.section`\n    max-width: 500px;\n\n    & h3{\n      border-color:${props => (props.result === 'wrong')? \"red\" :(props.result == 'right')? \"green\" : 'transparent'};\n      border-width: 0 0  0 5px;\n      border-style: solid;\n      padding: 5px 5px 5px 10px;\n    }\n`;\n\nexport default function Question(props) {\n  const { item } = props;\n  const [resultValue, setResult] = useState(null);\n\n  const context = useContext(Quizcontext);\n  const activeQuestion = context.state.questions[context.state.activeQuestion];\n\n \n  function submitAnswer(i){\n    let newState = context.state;\n    const result = (activeQuestion.answer.number == i)? \"right\" : \"wrong\";\n    setResult(result);\n    newState.results[result]++\n    newState.submitted = true;\n    context.dispatch({type:\"submitQuestion\",payload:newState})\n  }\n\n  return (\n    <QuestionContainer>\n      <h2>{item.question}</h2>\n      <Answers correct={activeQuestion.answer.number } submitted={context.state.submitted}>\n        {item.choices.map((c, i) => (\n          <div onClick={() => submitAnswer(i)} key={`choice-${i}`}>{c}</div>\n        ))}\n      </Answers>\n      <span>{context.state.activeQuestion + 1}/{context.state.questions.length} </span>\n      { context.state.submitted  &&\n        <ResultSection  result={resultValue} >\n          <h3>{context.state.questions[context.state.activeQuestion].answer.explaination}</h3>\n        </ResultSection>\n      }\n\n    </QuestionContainer>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"f67a02486f5159b315a27931a0a10ae2","cacheData":{"env":{}}}